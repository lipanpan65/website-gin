{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "üöÄ Go Gin Debug",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/cmd/main.go",
            "cwd": "${workspaceFolder}",
            "env": {
                "GO_ENV": "dev"
            },
            "console": "integratedTerminal",
            "showLog": true
        },
        {
            "name": "üîç Go Gin Remote Debug",
            "type": "go",
            "request": "attach",
            "mode": "remote",
            "remotePath": "${workspaceFolder}",
            "port": 2345,
            "host": "127.0.0.1",
            "showLog": true,
            "trace": "log"
        }
    ]
}